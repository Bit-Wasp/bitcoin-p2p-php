{
  "name": "bitwasp/bitcoin-p2p",
  "description": "Implementation of the bitcoin protocol using ReactPHP",
  "type": "library",
  "homepage": "https://github.com/bit-wasp/node-php",
  "license": "Unlicence",
  "authors": [
    {
      "name": "Thomas Kerin",
      "homepage": "https://thomaskerin.io",
      "role": "Author"
    }
  ],
  "autoload": {
    "psr-4": {
      "BitWasp\\Bitcoin\\Networking\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BitWasp\\Bitcoin\\Tests\\Networking\\": "tests/"
    }
  },
  "require": {
    "bitwasp/bitcoin": "~0.0.33.2",
    "react/socket-client": "~0.5.0",
    "react/socket": "~0.4.0",
    "react/dns": "~0.4.0",
    "clue/socks-react": "~0.4",
    "christian-riesen/base32": "~1.2"
  },
  "require-dev": {
    "ext-json": "*",
    "phpunit/phpunit": "^5.4.0",
    "squizlabs/php_codesniffer": "^2.0.0"
  }
}
